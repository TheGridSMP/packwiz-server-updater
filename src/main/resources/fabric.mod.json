{
  "schemaVersion": 1,
  "id": "packwizsu",
  "version": "${version}",

  "name": "PackWiz Server Updater",
  "description": "Updates packwiz modpacks server-side.",
  "authors": [
    "Sleepy Evelyn"
  ],
  "contact": {
    "homepage_old": "https://modrinth.com/mod/packwiz-server-updater",
    "sources": "https://github.com/TheGridSMP/packwiz-server-updater"
  },

  "license": "MIT",
  "icon": "assets/packwizsu/icon.png",

  "environment": "server",
  "entrypoints": {
    "server": [
      "sleepy_evelyn.packwizsu.PackWizSU"
    ]
  },
  "depends": {
    "fabricloader": ">=0.7.0",
    "fabric": "*",
    "minecraft": "~{minecraft_version}",
    "cloth-config": ">=${cloth_version}"
  },
  "mixins": [
    "packwizsu.mixins.json"
  ]
}
